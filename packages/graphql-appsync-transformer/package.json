{
  "name": "graphql-appsync-transformer",
  "version": "0.1.1",
  "description": "An AppSync model transform that takes a cloudformation document and writes mapping templates,lambda function, and schema if a path is specified.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rm -rf ./lib",
    "lint": "tslint -p ./tslint.json"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "author": "Lisa Shon",
  "license": "MIT",
  "dependencies": {
    "cloudform": "^2.2.1",
    "graphql": "^0.13.2",
    "graphql-elasticsearch-transformer": "^0.1.1",
    "graphql-mapping-template": "^0.1.1",
    "graphql-transform": "^0.1.1",
    "graphql-transformer-common": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
