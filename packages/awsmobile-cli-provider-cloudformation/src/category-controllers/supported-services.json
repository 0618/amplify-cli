{
	"storage" : {
		"S3": {
			"inputs": [
				{
					"key": "resourceName",
					"question": "Please provide a friendly name for your resource?",
					"required": true
				},
				{

					"key": "bucketName",
					"question": "What is the bucket name?",
					"required": true
				},
				{
				    "key": "policy",
				    "question": "What is the bucket name?",
				    "required": false,
				    "options": [
				    	"AuthenticatedRead", 
				    	"AwsExecRead", 
				    	"BucketOwnerRead", 
				    	"BucketOwnerFullControl", 
				    	"LogDeliveryWrite", 
				    	"Private", 
				    	"PublicRead", 
				    	"PublicReadWrite"]
				}
			],
			"cfnFilename": "s3-cloudformation-template.yml.ejs"
		}
	}
}