{
  "name": "graphql-transformers-e2e-tests",
  "version": "0.1.1",
  "description": "End to end functional tests for appsync supported transformers.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rm -rf ./lib",
    "lint": "tslint -p ./tslint.json"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "author": "Michael Paris",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "cloudform": "^2.2.1",
    "graphql": "^0.13.2",
    "graphql-mapping-template": "^0.1.1",
    "graphql-transform": "^0.1.1",
    "graphql-transformer-common": "^0.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/graphql": "^0.13.1",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "aws-amplify": "^1.0.4",
    "aws-sdk": "^2.259.1",
    "graphql-appsync-transformer": "^0.1.1",
    "graphql-auth-transformer": "^0.1.1",
    "graphql-connection-transformer": "^0.1.1",
    "graphql-dynamodb-transformer": "^0.1.1",
    "graphql-elasticsearch-transformer": "^0.1.1",
    "graphql-versioned-transformer": "^0.1.1",
    "jest": "^23.1.0",
    "node-fetch": "^2.2.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
