{
	"S3": {
		"inputs": [
			{
				"key": "resourceName",
				"question": "Please provide a friendly name for your resource that will be used to label this category in the project:",
				"validation": {
					"operator": "regex",
					"value": "^[a-zA-Z0-9]+$",
					"onErrorMsg": "Resource name should be alphanumeric"
				},
				"required": true
			},
			{

				"key": "bucketName",
				"question": "Please provide bucket name:",
				"validation": {
					"operator": "regex",
					"value": "^[a-z0-9]+$",
					"onErrorMsg": "Bucket name should be alphanumeric and lowercase"
				},
				"required": true
			},
			{
			    "key": "policy",
			    "type": "list",
			    "question": "Please provide a bucket policy:",
			    "required": true,
			    "options": [
			    	"AuthenticatedRead", 
			    	"AwsExecRead", 
			    	"BucketOwnerRead", 
			    	"BucketOwnerFullControl", 
			    	"LogDeliveryWrite", 
			    	"Private", 
			    	"PublicRead", 
			    	"PublicReadWrite"]
			}
		],
		"defaultValuesFilename": "s3-defaults.js",
		"cfnFilename": "s3-cloudformation-template.yml.ejs",
		"provider": "awsmobile-provider-cloudformation"
	}
}