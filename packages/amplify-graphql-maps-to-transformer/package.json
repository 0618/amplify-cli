    "watch": "tsc -w",
    "clean": "rimraf ./lib tsconfig.tsbuildinfo",
    "test": "jest"
  },
  "keywords": [
    "amplify",
    "graphql",
    "cloudformation",
    "aws",
    "original",
    "transformer"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-amplify/graphql-transformer-core": "0.9.2",
    "@aws-amplify/graphql-transformer-interfaces": "1.10.0",
