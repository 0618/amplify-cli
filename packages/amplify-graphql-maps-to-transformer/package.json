  },
  "jest": {
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
