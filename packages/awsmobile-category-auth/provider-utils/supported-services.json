{
	"Cognito": {
		"inputs": [
			{
				"key": "resourceName",
				"question": "Please provide a friendly name for your resource?",
				"required": true
			},
			{
				"key": "userPoolName",
				"question": "Please provide a friendly name for your user pool.",
				"required": true
			},
			{
				"key": "mfaConfiguration",
				"question": "Please choose an MFA Configuration:",
				"required": true,
				"options": [
					"ON",
					"OFF",
					"OPTIONAL"]
			},
			{
				"key": "sms",
				"question": "Am I being asked?",
				"conditions": [
					{
						"operator": "||",
						"conditions": [
							{
								"operator": "||",
								"conditions": [
									{
										"key": "resourceName",
										"value": "a",
										"operator": "!="
									},
									{
										"key": "userPoolName",
										"value": "b",
										"operator": "!="
									}
								]
							},
							{
								"key": "mfaConfiguration",
								"value": "ON",
								"operator": "="
							}
						]
					}	
				]
			}
		],
		"cfnFilename": "cognito-userpool-template.yml.ejs",
		"provider": "awsmobile-provider-cloudformation"
	}
}