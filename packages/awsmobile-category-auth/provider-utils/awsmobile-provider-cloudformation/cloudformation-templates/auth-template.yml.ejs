Resources:
  <%if (props.authSelections.includes('Cognito User Pools')) { %>
  UserPool:
    Type: AWS::Cognito::UserPool
    Properties:
      UserPoolName: <%= props.userPoolName %>
      MfaConfiguration: '<%= props.mfaConfiguration.toString() %>'
      <%if (props.mfaConfiguration != 'OFF') { %>
        SmsConfiguration: 
        SnsCallerArn: <%= props.snsCallarArn %>
        ExternalId: <%= props.externalId %>
        <% } %>
  <% } %>
  <%if (props.authSelections.includes('Cognito Identity Pools')) { %>
  IdentityPool:
    Type: AWS::Cognito::IdentityPool
    Properties: 
      IdentityPoolName: <%= props.identityPoolName %>
  <% } %> 
Outputs:
  UserPoolName:
    Value: <%= props.userPoolName %>
    Description:  name for the user pool